{
  "name": "matchmore",
  "version": "0.7.0",
  "description": "We_connect_things",
  "license": "MIT",
  "main": "dist/index.js",
  "homepage": "https://matchmore.io",
  "scripts": {
    "build": "tsc",
    "test": "./node_modules/.bin/mocha --compilers ts:ts-node/register ./test/**/*.ts",
    "browserify": "browserify ./dist/index.js --standalone matchmore > ./dist/web/matchmore.js",
    "reactify": "browserify -t [ babelify --presets [ react-native-stage-0 ] ] ./lib/src/manager.js --standalone matchmore > ./build/matchmore-react.js",
    "minify": "browserify ./lib/src/manager.js --standalone matchmore | uglifyjs -cm > ./build/matchmore.min.js"
  },
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/chai-as-promised": "^7.1.0",
    "@types/mocha": "^2.2.48",
    "babel-core": "^6.26.0",
    "browserify": "^16.1.1",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "mocha": "^3.2.0",
    "ts-node": "^5.0.1",
    "typescript": "^2.7.2"
  },
  "dependencies": {
    "Base64": "^1.0.1",
    "matchmore_alps_core_rest_api": "file:./matchmore_alps_core_rest_api",
    "websocket": "^1.0.25"
  }
}
